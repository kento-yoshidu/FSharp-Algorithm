// https://atcoder.jp/contests/joi2026yo1c/tasks/joi2026_yo1c_d

let fn _n _m a =
    a
    |> Array.map Array.max
    |> Array.sum

printfn "%i" (fn 3 2 [| [| 4; 7 |]; [| 9; 6 |]; [| 5; 5 |] |])
//=> 21

printfn "%i" (fn 1 5 [| [| 2; 8; 3; 6; 1 |] |])
//=> 8

printfn "%i" (fn 4 1 [| [| 6 |]; [| 9 |]; [| 1 |]; [| 8 |] |])
//=> 24

printfn "%i" (fn 5 6 [| [| 91; 30; 75; 5; 81; 16 |]; [| 82; 26; 5; 76; 91; 94 |]; [| 36; 38; 44; 81; 43; 65 |]; [| 4; 63; 68; 14; 100; 27 |]; [| 8; 54; 17; 36; 64; 87 |] |])
//=> 453
